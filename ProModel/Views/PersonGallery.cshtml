@using WebExplorer.Models
@inherits Umbraco.Web.Mvc.UmbracoViewPage<PersonGalleryModel>
@{
    Layout = "WebBase.cshtml";

}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Explorer</title>
    <meta name="description" content="Explorer Website.">
    <meta name="keywords" content="The Explorer website.">

    <link href="https://fonts.googleapis.com/css?family=Abel" rel="stylesheet">

    <link href="~/css/styles-merged.css" rel="stylesheet" />

    <link href="~/css/style.min.css" rel="stylesheet" />

    <link href="~/css/custom.css" rel="stylesheet" />
    <script src="~/scripts/html5shiv.min.js"></script>
    <script src="~/scripts/vendor/respond.min.js"></script>
</head>

<section id="work" data-stellar-background-ratio="0.5">
    <div>
        <div class="container">
            <div class="row">

                <div class="col-md-12 col-sm-12">
                    <div class="section-title">
                        <br />
                        <br />
                        <h2><font color="#003000">Nossa Galeria</font></h2>
                    </div>
                </div>
                <br />
                <br />
                @foreach (var umbUser in Model.UmbUsers)
                {
                    <img src="@(umbUser.ImagemUrl)" class="img-responsive" alt="Work">

                    <div class="work-info">
                        <h3>@umbUser.Nome</h3>
                    </div>



                }
            </div>

        </div>
    </div>
</section>
<script src="~/scripts/scripts.min.js"></script>

<script src="~/scripts/main.min.js"></script>

<script src="~/scripts/custom.js"></script>
